    "element-ui": "^2.13.2",
    "lodash": "^4.17.19",
    "normalize.css": "^8.0.1",
    "tinymce": "^5.4.2",
    "vue": "^2.6.11",
    "vuedraggable": "^2.24.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
